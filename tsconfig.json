{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "target": "esnext",
    // "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "strict": false
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/api/tournaments/route.ts",
    "src/app/api/tournaments/join/route.js"
, "src/app/api/fitbitLogin/route.js", "src/app/api/games/[code]/join/route.js", "src/app/api/games/[code]/route.js", "src/app/api/waitlist/route.js"  ],
  "exclude": [
    "node_modules"
  ]
}
